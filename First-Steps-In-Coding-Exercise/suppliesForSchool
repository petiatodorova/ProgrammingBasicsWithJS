function supplies(input){
    const penPrice = 5.80;
    const markersPrice = 7.20;
    const preparatPrice = 1.20;
    let penPackets = parseInt(input[0]);
    let markersPackets = parseInt(input[1]);
    let preparatLitres = parseInt(input[2]);
    let percentDiscount = parseInt(input[3])/100;
    let moneyNeeded = (1 - percentDiscount) * (penPackets * penPrice + markersPackets * markersPrice + preparatLitres * preparatPrice);
    console.log(moneyNeeded);

}
